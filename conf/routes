# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

# CoffeeBear

GET /user   controllers.Application.user
POST /userCreate  controllers.Application.userCreate
POST /userFindByName  controllers.Application.userFindByName
POST /userFindById   controllers.Application.userFindById
POST /userFindByPayment   controllers.Application.userFindByPayment
PUT /userUpdate   controllers.Application.userUpdate
DELETE /userDelete   controllers.Application.userDelete
